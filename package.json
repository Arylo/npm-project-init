{
  "name": "arylo-init",
  "version": "2.1.19",
  "description": "Initial Node.js Project for Arylo Edition",
  "main": "./dist/index.js",
  "bin": {
    "arylo-init": "./bin/arylo-init"
  },
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "rimraf dist",
    "resource": "node ./scripts/copy",
    "tsc": "tsc --locale zh-cn --pretty",
    "build": "npm run tsc -- -P tsconfig.json",
    "build:prod": "npm run tsc -- -P tsconfig.prod.json",
    "build:test": "npm run tsc -- -P tsconfig.test.json",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint lib/**/*.ts public/**/*.ts",
    "lint:javascript": "eslint ./**/*.js",
    "pretest": "npm run clean && npm run build:test && npm run resource",
    "test": "ava ./dist/test/**/*.spec.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arylo/npm-project-init.git"
  },
  "keywords": [
    "arylo",
    "init",
    "cli",
    "cli-app",
    "easy to use",
    "tool",
    "generator",
    "generate",
    "scaffold",
    "command",
    "command-line",
    "template",
    "codemod"
  ],
  "files": [
    "bin",
    "dist",
    "lib"
  ],
  "author": "AryloYeung <arylo.open@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arylo/npm-project-init/issues"
  },
  "homepage": "https://github.com/Arylo/npm-project-init#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "ftconfig": "^1.1.0",
    "js-yaml": "^3.12.0",
    "make-dir": "^1.3.0",
    "rimraf": "^2.6.2",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/lint": "^7.2.1",
    "@types/glob": "^7.1.1",
    "@types/js-yaml": "^3.11.2",
    "@types/make-dir": "^1.0.3",
    "@types/node": "^10.1.2",
    "@types/request": "^2.47.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/rimraf": "^2.0.2",
    "@types/update-notifier": "^2.2.0",
    "@types/yaml": "^1.0.1",
    "ava": "^1.0.1",
    "dtss": "^1.0.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "glob": "^7.1.3",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "md5": "^2.2.1",
    "prettier": "^1.14.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "tslint": "^5.10.0",
    "typescript": "^3.1.3",
    "y-config": "^2.0.1"
  }
}
